<h1>Chat</h1>
<h2>Add an message</h2>

<form method="POST">
    Sender: <input type="text" name="sender" /><br/>
    Message: <input type="text" name="message" /><br/>
    <input type="submit" value="Submit!" />
</form>

<h2>Latest Messages</h2>
<ul>
    <% it.message.forEach((obj) => { %>
        <li><%= obj.sender %>: <%= obj.message %> </li>
    <% }) %>
</ul>